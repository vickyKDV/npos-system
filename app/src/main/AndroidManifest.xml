<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
package="com.nibiru.billingkaraoke">

<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.VIBRATE" />

<application
    android:name=".utils.PrefsApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Login"
    tools:ignore="GoogleAppIndexingWarning"
    tools:targetApi="donut">
    <activity
        android:name=".Activity.MainActivity"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.LoginActivity"
        android:theme="@style/Login"
        tools:ignore="InnerclassSeparator">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".Activity.CategoryActivity"
        android:label="@string/title_activity_category"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.SubCategoryActivity"
        android:label="@string/title_activity_category"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.MenuActivity"
        android:label="@string/title_activity_menu"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.OrderProdukActivity"
        android:theme="@style/Dialog"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.UpdateOrderActivity"
        android:theme="@style/Dialog"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name=".Activity.DetailOrderActivity"
        android:theme="@style/Dialog"
        tools:ignore="InnerclassSeparator" />
    <activity
        android:name="com.nibiru.billingkaraoke.Activity.ReportMainActivity"
        android:label="@string/title_activity_report_main"
        android:theme="@style/AppTheme.Template.Base" />
</application>

</manifest>


